	<!-- Row start -->
    <div class="row gutters">
        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            
            <div class="table-container">
                <div class="t-header" style="background-color: #e29a00; color: azure;">قائمة القضايا</div>
                <div class="table-responsive">
                    <table id="copy-print-csv" class="table custom-table">
                        <thead style="background-color: #e29a0050">
                            <tr>
                                <th>اسم الموكل</th>
                                <th>الوضع القانوني</th>
                                <th>الخصم</th>
                                <th>نوع القضية</th>
                                <th>المحكمة</th>
                               
            
                            </tr>
                        </thead>
                        <tbody>
                            <% lawcases.forEach(lawcase => { %>
                            
                                
                            
                                
                                <tr>
                                    <td><a href="/lawcases/<%= lawcase.id %>"><%= lawcase.customer.name %></a></td>
                                    <td><%= lawcase.customerStatus %></td>
                                    <td><%= lawcase.opponent %></td>
                                    <td><%= lawcase.caseType %></td>
                                    <td><%= lawcase.court %></td>
                                    
                                </tr>
                            
                               
                           
                            <% }) %>
                           
                            
                        </tbody>
                    </table>
                </div>
            </div>